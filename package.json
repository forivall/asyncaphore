{
  "name": "$NAME",
  "description": "$DESCRIPTION",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "lint": "xo --ignore {test,lib}/**",
    "build": "babel src -d lib",
    "dev-watch": "babel src -d lib -s -w",
    "inspect": "jsinspect",
    "pretest": "npm run lint && npm run inspect",
    "test": "ava",
    "test-cov": "nyc ava",
    "posttest-cov": "",
    "preversion": "npm run build && npm run lint && npm run test"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Jordan Klassen <forivall@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "jsinspect": "^0.7.0",
    "nyc": "^5.5.0",
    "xo": "^0.12.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:forivall/errmsg.git"
  },
  "babel": {
    "presets": ["es2015"]
  },
  "xo": {
    "space": 2,
    "rules": {
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ]
    },
    "overrides": [
      {
        "files": "test/**",
        "esnext": true,
        "semicolon": false
      }
    ]
  }
}
